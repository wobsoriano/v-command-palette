{
  "name": "v-command-palette",
  "type": "module",
  "version": "0.0.17",
  "packageManager": "pnpm@8.15.4",
  "description": "A command palette for Vuetify.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/v-command-palette#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/v-command-palette.git"
  },
  "bugs": "https://github.com/wobsoriano/v-command-palette/issues",
  "keywords": [
    "vuetify",
    "vue",
    "command",
    "palette"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && vue-tsc --noEmit && vite build --mode lib && rimraf dist/vite.svg",
    "dev": "vite serve",
    "dev:build": "vite build",
    "dev:preview": "vite preview",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "vue": "^3.2.0",
    "vuetify": "^3.3.0"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.0",
    "fuse.js": "^7.0.0",
    "nanoid": "^5.0.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@mdi/font": "^7.4.47",
    "@types/node": "^20.11.24",
    "@vitejs/plugin-vue": "^5.0.4",
    "bumpp": "^9.3.0",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.5",
    "roboto-fontface": "^0.10.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-vuetify": "^2.0.2",
    "vue": "^3.4.21",
    "vue-tsc": "^1.8.27",
    "vuetify": "^3.5.6"
  }
}
